<h1>Listing users</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Login ID</th>
    <th>Level</th>
    <th>Email</th>
    <th>Password</th>
    <th>Name</th>
    <th>Twitter</th>
    <th>Birthday</th>
    <th>Blog url</th>
    <th>Website url</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=h user.id %></td>
    <td><%=h user.login_id %></td>
    <td><%=h user.level %></td>
    <td><%=h user.email %></td>
    <td><%=h user.password %></td>
    <td><%=h user.name %></td>
    <td><%=h user.twitter_id %></td>
    <td><%=h user.birthday %></td>
    <td><%=h user.blog_url %></td>
    <td><%=h user.website_url %></td>
    <td><%= link_to 'Show', :action => "show", :id => user.id %></td>
    <td><%= link_to 'Edit', :action => "edit", :id => user.id %></td>
    <td><%= link_to 'Destroy', { :action => "destroy", :id => user.id }, :confirm => "[ #{user.login_id} ]を削除します。" %></td>
  </tr>
<% end %>
</table>

<br />

<%#= link_to 'New user', new_user_path %>